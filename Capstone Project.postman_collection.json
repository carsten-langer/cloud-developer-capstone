									"const schema = {\r",
									"    \"$schema\": \"http://json-schema.org/draft-07/schema#\",\r",
									"    \"type\": \"object\",\r",
									"    \"required\": [\r",
									"        \"recipes\"\r",
									"    ],\r",
									"    \"additionalProperties\": false,\r",
									"    \"properties\": {\r",
									"        \"recipes\": {\r",
									"            \"type\": \"array\",\r",
									"            \"items\": {\r",
									"                \"type\": \"object\",\r",
									"                \"required\": [\r",
									"                    \"userId\",\r",
									"                    \"recipeId\",\r",
									"                    \"name\",\r",
									"                    \"recipe\",\r",
									"                    \"attachments\"\r",
									"                ],\r",
									"                \"additionalProperties\": false,\r",
									"                \"properties\": {\r",
									"                    \"userId\": {\r",
									"                        \"type\": \"string\",\r",
